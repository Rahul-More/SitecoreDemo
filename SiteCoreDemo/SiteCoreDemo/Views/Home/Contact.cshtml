@using SiteCoreDemo.Areas.Admin.Models;
@using System.Web;
@model PagesData
@{
    ViewBag.Title = Model.PageTitle;
    ViewBag.Keywords = Model.Keywords;
    ViewBag.Description = Model.Descriptions;
}


<section id="featured" class="bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="row">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d109706.98230125969!2d76.70496638734087!3d30.764945783713284!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x390ff27f67661afb%3A0xad4389e0eee19a83!2sPost+Graduate+Institute+of+Medical+Education+%26+Research%2C+Chandigarh!5e0!3m2!1sen!2sin!4v1533145488105" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                </div>
            </div>

            <div class="col-lg-6">
                <div id="content" class="row text-justify" style="padding:1em;">
                    <div class="col-sm-12 col-lg-12">
                        @Html.Raw(HttpUtility.HtmlDecode(Model.PageContent))
                    </div>
                </div>
            </div>
        </div>
    </div>



</section>
